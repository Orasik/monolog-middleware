{
  "name": "oras/monolog-middleware",
  "description": "PSR-7 Monolog Middleware",
  "license": "MIT",
  "authors": [
    {
      "name": "Oras Al-Kubaisi",
      "email": "code@oras.me"
    }
  ],
  "require": {
    "php": "^5.5 || ^7.0",
    "monolog/monolog": "^1.19",
    "psr/http-message": "~1.0.0",
    "zendframework/zend-stratigility": "^2",
    "container-interop/container-interop": "^1.1",
    "symfony/http-foundation": "^3.0",
    "guzzlehttp/psr7": "^1.3 || ^1.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "MonologMiddleware\\": "src/MonologMiddleware/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MonologMiddleware\\": "src/MonologMiddleware/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
